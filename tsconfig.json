{
	"extends": "astro/tsconfigs/base",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@assets/*": [
				"src/assets/*"
			],
			"@components/*": [
				"src/components/*"
			],
			"@layouts/*": [
				"src/layouts/*"
			],
			"@lib/*": [
				"src/lib/*"
			]
		},
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"allowJs": true
	}
}
